                            .collapse-toggle__icon
                                i.fas.fa-chevron-up
                        .collapse-content
                            .row
                                .col-12.col-lg-6.input-container--with-popover
                                    label.form__label
                                        strong User Monitoring
                                    .input-container
                                        .switch
                                            input(type="checkbox" checked="checked")
                                            span.slider
                                                .switch__text.switch__text--on ON
                                                .switch__text.switch__text--off OFF
                                        .row.popover--internal.show: .col-12
