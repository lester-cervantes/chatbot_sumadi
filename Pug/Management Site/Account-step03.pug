extend includes/layout-normal.pug
block main-content
    .container
        .row
            .col-12.section--boxed
                .title.title--medium Add New Account
                +wizard({'activeStep': 3})
                .wizard-content
                    .row
                        .col-12.col-lg-6
                            label.form__label
                                strong Google Analytics Track ID
                            .input-container
                                input(type="text" placeholder="UA-123434543543-4")
                        .col-12.col-lg-6
                            label.form__label
                                strong AWS - Rekonigtion Collection ID
                            .input-container
                                input(type="text" placeholder="SU2020")
                    .collapse.collapse--open
                        a.collapse-toggle
